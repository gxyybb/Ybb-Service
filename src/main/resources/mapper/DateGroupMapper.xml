<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.ybb.mapper.DateGroupMapper">

    <resultMap id="BaseResultMap" type="org.example.ybb.domain.DateGroup">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="date" column="date" jdbcType="INTEGER"/>
            <result property="completionStatus" column="completion_status" jdbcType="TINYINT"/>
            <result property="userId" column="user_id" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,date,completion_status,
        user_id
    </sql>
</mapper>
